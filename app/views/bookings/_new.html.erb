<div class="container-rent">
  <div id='book-booking-dates' data-unavailable="<%= @book.unavailable_dates.to_json %>">
   <%= simple_form_for([@book, @booking]) do |f| %>
       <%= f.input :start_date, as: :string, type: :text %>
       <%= f.input :end_date, as: :string, type: :text %>
       <%= f.submit "Book", class: "button-show" %>
    <% end %>
   </div>
 </div>



